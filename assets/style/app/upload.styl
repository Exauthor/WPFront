.input--upload
  label
    text-indent 10px
  .highlight__line--bottom
    width calc(100% - 20px)

.upload-title
  max-width calc(100% - 50px)
  white-space nowrap
  overflow hidden
  text-overflow ellipsis


.upload-block
  background color-main--dark
  width input-height
  height input-height
  float right
  border-radius 50%
